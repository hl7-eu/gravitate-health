* section.section[=].section[=].text.status = #additional
* section.section[=].section[=].text.div = "<div xmlns=\"http://www.w3.org/1999/xhtml\">\n <div class=\"paragraph\">Os comprimidos revestidos por película de\n                              Glucophage 500 mg são brancos, circulares e convexos.<br/>Os\n                              comprimidos são fornecidos em embalagens com blister de 1 (x100), 9,\n                              20, 21, 30,<br/>40, 50, 56, 60, 84, 90, 100, 120, 200, 500, 600 ou\n                              1000 comprimidos ou em frascos de<br/>plástico, com tampa resistente à\n                              abertura por crianças, de 21, 30, 40, 50, 60, 100, 120,<br/>300, 400,\n                              500, 600 ou 1000 comprimidos.</div>\n                           <div class=\"paragraph\">É possível que não sejam comercializadas todas as\n                              apresentações.</div>\n                        </div>"
